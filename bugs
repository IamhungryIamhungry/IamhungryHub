local d = game:GetService("RunService")
local e = game:GetService("UserInputService")
local f = game:GetService("CoreGui")
local g = game:GetService("TweenService")
local h = game:GetService("TeleportService")
local i = game:GetService("HttpService")

local j = workspace.CurrentCamera
local k = c.LocalPlayer

local l = false -- Aimbot 활성화 상태
local m = false -- ESP 활성화 상태
local n = false -- 팀 체크 활성화 상태
local o = false -- 마우스 버튼 누름 상태
local p = {} -- ESP 객체 저장 테이블
local q = Color3.fromRGB(255, 0, 0) -- ESP 색상

local r = false -- AntiBan 활성화 상태
local s, t -- AntiBan 메타테이블 변수

-- AntiBan 기능 활성화
local function AntiBanOn()
    if r then return end
    r = true
    
    -- 필수 환경 함수 확인
    if not (getrawmetatable and hookmetamethod and getnamecallmethod) then
        print("[AntiBan] Some environment functions missing.")
        return
    end
    
    -- 메타메서드 후킹 (Kick 방지)
    t = getrawmetatable(game)
    s = hookmetamethod(game, "__namecall", function(self, ...)
        local method = getnamecallmethod()
        if method == "Kick" then
            print("[AntiBan] Blocked Kick call!")
            return nil
        end
        return s(self, ...)
    end)
    
    print("[AntiBan] ON - Hooked Kick calls.")
end

-- AntiBan 기능 비활성화
local function AntiBanOff()
    if not r then return end
    r = false
    
    if t and s then
        t.__namecall = s
        print("[AntiBan] OFF - Restored metamethod.")
    end
end

-- 기존 UI 정리
for _, v in ipairs(f:GetChildren()) do
    if v.Name == "AimbotKeyUI" or v.Name == "AimbotGUI" then
        v:Destroy()
    end
end

for _, v in ipairs(f:GetChildren()) do
    if v:IsA("BoxHandleAdornment") or v:IsA("BillboardGui") then
        v:Destroy()
    end
end

-- 플레이어 팀 정보 가져오기 함수
local function getPlayerTeam(player)
    if player.Team then
        return player.Team.Name
    end
    
    local leaderstats = player:FindFirstChild("leaderstats")
    if leaderstats then
        local teamValue = leaderstats:FindFirstChild("Team")
        if teamValue and (teamValue:IsA("StringValue") or teamValue:IsA("IntValue")) then
            return tostring(teamValue.Value)
        end
    end
    
    local teamAttribute = player:GetAttribute("Team")
    if teamAttribute then
        return tostring(teamAttribute)
    end
    
    return "(NoTeam)"
end

-- 같은 팀인지 확인하는 함수
local function isSameTeam(player)
    if not n then return false end
    
    local myTeam = getPlayerTeam(k)
    local playerTeam = getPlayerTeam(player)
    
    print("[TEAM CHECK] MyTeam=", myTeam, " vs PlayerTeam=", playerTeam)
    return (myTeam == playerTeam)
end

-- 서버 호핑 함수
local function doServerHop()
    local placeId = game.PlaceId
    local url = ("https://games.roblox.com/v1/games/%d/servers/Public?sortOrder=Asc&limit=100"):format(placeId)
    
    local success, result = pcall(function()
        return i:JSONDecode(game:HttpGet(url))
    end)
    
    if not success or not result or not result.data then
        print("[ServerHop] Could not fetch servers or data missing.")
        return
    end
    
    for _, server in ipairs(result.data) do
        if server.id ~= game.JobId and server.playing < server.maxPlayers then
            print("[ServerHop] Found server:", server.id)
            h:TeleportToPlaceInstance(placeId, server.id, k)
            return
        end
    end
    
    print("[ServerHop] No suitable server found.")
end

