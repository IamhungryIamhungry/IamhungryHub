        local enabled = false
        button.MouseButton1Click:Connect(function()
            enabled = not enabled
            button.BackgroundColor3 = enabled and Color3.fromRGB(0, 160, 0) or Color3.fromRGB(70, 70, 70)
            callback(enabled)
            statusLabel.Text = "Status: " .. text .. (enabled and " Enabled" or " Disabled")
        end)
        
        return button
    end
    
    -- 각 기능 버튼 생성
    local aimbotBtn = createToggleButton("Aimbot", 110, function(enabled)
        l = enabled
        print("[DEBUG] Aimbot set to: " .. tostring(l))
    end)
    
    local espBtn = createToggleButton("ESP", 150, function(enabled)
        m = enabled
        print("[DEBUG] ESP set to: " .. tostring(m))
        task.spawn(function() updateAllESP() end)
    end)
    
    local teamCheckBtn = createToggleButton("Team Check", 190, function(enabled)
        n = enabled
        print("[DEBUG] Team Check set to: " .. tostring(n))
        task.spawn(function() updateAllESP() end)
    end)
    
    local antiBanBtn = createToggleButton("AntiBan", 230, function(enabled)
        if enabled then
            AntiBanOn()
        else
            AntiBanOff()
        end
    end)
    
    -- 팀 체크 기본값 설정
    teamCheckBtn.MouseButton1Click:Fire()  -- 팀 체크 활성화
    
    -- 서버 호핑 프레임
    local hopFrame = Instance.new("Frame")
    hopFrame.Size = UDim2.new(0, 200, 0, 30)
    hopFrame.Position = UDim2.new(0.5, 0, 0, 270)
    hopFrame.AnchorPoint = Vector2.new(0.5, 0)
    hopFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    hopFrame.BorderSizePixel = 0
    hopFrame.Name = "HopFrame"
    hopFrame.Parent = contentFrame
    
    local hopCorner = Instance.new("UICorner")
    hopCorner.CornerRadius = UDim.new(0, 8)
    hopCorner.Parent = hopFrame
    
    local hopArrow = Instance.new("TextButton")
    hopArrow.Size = UDim2.new(0, 20, 0, 20)
    hopArrow.Position = UDim2.new(0, 5, 0, 5)
    hopArrow.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
    hopArrow.Text = "▶"
    hopArrow.Font = Enum.Font.SourceSansBold
    hopArrow.TextColor3 = Color3.fromRGB(255, 255, 255)
    hopArrow.TextSize = 16
    hopArrow.Name = "ServerHopArrow"
    hopArrow.Parent = hopFrame
    
    local arrowCorner = Instance.new("UICorner")
    arrowCorner.CornerRadius = UDim.new(1, 0)
    arrowCorner.Parent = hopArrow
    
    local hopButton = Instance.new("TextButton")
    hopButton.Size = UDim2.new(0, 165, 0, 20)  -- 더 넓게 수정
    hopButton.Position = UDim2.new(0, 30, 0, 5)
    hopButton.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
    hopButton.Text = "Server Hop"
    hopButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    hopButton.Font = Enum.Font.SourceSansSemibold
    hopButton.TextSize = 14
    hopButton.Name = "ServerHopBtn"
    hopButton.Parent = hopFrame
    
    local hopButtonCorner = Instance.new("UICorner")
    hopButtonCorner.CornerRadius = UDim.new(0, 6)
    hopButtonCorner.Parent = hopButton
    
    -- 서버 호핑 UI 토글
    local isHopCollapsed = true
    hopArrow.MouseButton1Click:Connect(function()
        isHopCollapsed = not isHopCollapsed
        local tweenInfo = TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out)
        
        if isHopCollapsed then
            hopArrow.Text = "▶"
            hopButton.Visible = false
            local tween = g:Create(hopFrame, tweenInfo, {Size = UDim2.new(0, 30, 0, 30)})
            tween:Play()
        else
            hopArrow.Text = "▼"
            local tween = g:Create(hopFrame, tweenInfo, {Size = UDim2.new(0, 200, 0, 30)})
            tween:Play()
            tween.Completed:Connect(function()
                hopButton.Visible = true
            end)
        end
    end)
    
    -- 서버 호핑 버튼 기능
    hopButton.MouseButton1Click:Connect(function()
        print("[ServerHop] Starting server hop...")
        statusLabel.Text = "Status: Server Hopping..."
        doServerHop()
    end)
    
    -- UI 크기 조절 기능
    local resizer = Instance.new("Frame")
    resizer.Size = UDim2.new(0, 16, 0, 16)
    resizer.AnchorPoint = Vector2.new(1, 1)
    resizer.Position = UDim2.new(1, 0, 1, 0)
    resizer.BackgroundTransparency = 1
    resizer.Name = "Resizer"
    resizer.Parent = mainFrame
    
    local isResizing = false
    local lastMousePos
    
    resizer.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            isResizing = true
            lastMousePos = input.Position
        end
    end)
    
    resizer.InputChanged:Connect(function(input)
        if isResizing and input.UserInputType == Enum.UserInputType.MouseMovement then
            local delta = input.Position - lastMousePos
            lastMousePos = input.Position
            mainFrame.Size = mainFrame.Size + UDim2.new(0, delta.X, 0, delta.Y)
        end
    end)
    
    resizer.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            isResizing = false
        end
    end)
    
    -- UI 토글 키 (Delete)
    local guiVisible = true
    e.InputBegan:Connect(function(input, gameProcessed)
        if gameProcessed then return end
        
        if input.KeyCode == Enum.KeyCode.Delete then
            guiVisible = not guiVisible
            mainFrame.Visible = guiVisible
        end
    end)
    
    -- 테두리 애니메이션
    task.spawn(function()
        local hue = 0
        while task.wait(0.02) do
            hue = (hue + 1) % 360
            stroke.Color = Color3.fromHSV(hue/360, 0.8, 1)
        end
    end)
    
    -- 가장 가까운 적 찾기 함수
    local function getClosestEnemy()
        if not l then return nil end
        
        local closestDistance, closestHead = math.huge, nil
        
        for _, player in ipairs(c:GetPlayers()) do
            if player ~= k and player.Character and 
               player.Character:FindFirstChild("Head") and 
               player.Character:FindFirstChild("Humanoid") then
                
                local humanoid = player.Character.Humanoid
                
                if humanoid.Health > 0 then
                    -- 팀 체크
                    if not (n and isSameTeam(player)) then
                        local head = player.Character.Head
                        local pos, onScreen = j:WorldToViewportPoint(head.Position)
                        
                        if onScreen then
                            local distance = (Vector2.new(pos.X, pos.Y) - Vector2.new(j.ViewportSize.X/2, j.ViewportSize.Y/2)).Magnitude
                            
                            if distance < closestDistance then
                                closestDistance = distance
                                closestHead = head
                            end
                        end
                    end
                end
            end
        end
        
        return closestHead
    end
    
    -- 에임 함수
    local function aimAt(target)
        if target then
            j.CFrame = CFrame.new(j.CFrame.Position, target.Position)
        end
    end
    
    -- 에임 활성화 키 (마우스 우클릭)
    e.InputBegan:Connect(function(input, gameProcessed)
        if gameProcessed then return end
        
        if input.UserInputType == Enum.UserInputType.MouseButton2 then  -- 우클릭으로 변경
            o = true
            print("[DEBUG] Aiming activated (mouse down)")
        end
    end)
    
    e.InputEnded:Connect(function(input, gameProcessed)
        if gameProcessed then return end
        
        if input.UserInputType == Enum.UserInputType.MouseButton2 then
            o = false
            print("[DEBUG] Aiming deactivated (mouse up)")
        end
    end)
    
    -- 에임봇 루프
    d:BindToRenderStep("AimbotLoop", Enum.RenderPriority.Camera.Value, function()
        if o and l then
            local target = getClosestEnemy()
            if target then
                aimAt(target)
            end
        end
    end)
    
    -- ESP 제거 함수
    local function removeESP(player)
        if p[player.UserId] then
            for _, obj in ipairs(p[player.UserId]) do
                if obj and obj.Parent then
                    obj:Destroy()
                end
            end
            p[player.UserId] = nil
        end
    end
    
    -- ESP 생성 함수
    local function createESP(player)
        if player == k then return end
        
        removeESP(player)
        
        if not m then 
            print("[DEBUG] ESP disabled, not creating ESP for: " .. player.Name)
            return 
        end
        
        if n and isSameTeam(player) then
            print("[DEBUG] Skipping ESP for teammate: " .. player.Name)
            return
        end
        
        if not player.Character then 
            print("[DEBUG] No character for player: " .. player.Name)
            return 
        end
        
        print("[DEBUG] Creating ESP for: " .. player.Name)
        
        local espObjects = {}
        p[player.UserId] = espObjects
        
        for _, part in ipairs(player.Character:GetChildren()) do
            if part:IsA("BasePart") then
                local box = Instance.new("BoxHandleAdornment")
                box.Size = part.Size + Vector3.new(0.1, 0.1, 0.1)
                box.Adornee = part
                box.Color3 = q
                box.Transparency = 0.5
                box.ZIndex = 0
                box.AlwaysOnTop = true
                box.Parent = f
                table.insert(espObjects, box)
            end
        end
        
        local head = player.Character:FindFirstChild("Head")
        if head then
            local billboard = Instance.new("BillboardGui")
            billboard.Size = UDim2.new(0, 200, 0, 50)
            billboard.Adornee = head
            billboard.StudsOffset = Vector3.new(0, 2, 0)
            billboard.AlwaysOnTop = true
            billboard.Parent = f
            
            local nameLabel = Instance.new("TextLabel")
            nameLabel.Size = UDim2.new(1, 0, 1, 0)
            nameLabel.BackgroundTransparency = 1
            nameLabel.TextColor3 = q
            nameLabel.Text = player.Name
            nameLabel.TextScaled = true
            nameLabel.Font = Enum.Font.SourceSansBold
            nameLabel.Parent = billboard
            
            table.insert(espObjects, billboard)
        end
    end
    
    -- 모든 플레이어 ESP 업데이트
    function updateAllESP()
        print("[DEBUG] Updating all ESP, ESP enabled: " .. tostring(m) .. ", Team Check: " .. tostring(n))
        
        for _, player in ipairs(c:GetPlayers()) do
            if player ~= k then  -- 로컬 플레이어 제외
                print("[DEBUG] Processing ESP for: " .. player.Name)
                removeESP(player)  -- 기존 ESP 제거
                
                if m then  -- ESP가 활성화된 경우에만 생성
                    createESP(player)
                end
            end
        end
    end
    
    -- 사용 시간 표시 업데이트
    task.spawn(function()
        while task.wait(1) do
            local elapsed = os.time() - startTime
            local minutes = math.floor(elapsed / 60)
            local seconds = elapsed % 60
            
            if timeLabel then
                timeLabel.Text = string.format("Time Opened: %dm %ds", minutes, seconds)
            end
        end
    end)
    
    -- 플레이어 캐릭터 추가 이벤트 핸들러
    local function onCharacterAdded(player)
        print("[DEBUG] Character added for: " .. player.Name)
        task.wait(0.5)  -- 캐릭터 로드 대기
        createESP(player)
    end
    
    -- 초기 ESP 설정 및 이벤트 연결
    for _, player in ipairs(c:GetPlayers()) do
        if player ~= k and player.Character then
            createESP(player)
        end
        
        player.CharacterAdded:Connect(function()
            onCharacterAdded(player)
        end)
    end
    
    -- 플레이어 참가/퇴장 이벤트 처리
    c.PlayerAdded:Connect(function(player)
        print("[DEBUG] Player added: " .. player.Name)
        player.CharacterAdded:Connect(function()
            onCharacterAdded(player)
        end)
    end)
    
    c.PlayerRemoving:Connect(function(player)
        print("[DEBUG] Player removing: " .. player.Name)
        removeESP(player)
    end)
    
    -- 주기적 ESP 업데이트
    task.spawn(function()
        while task.wait(3) do
            updateAllESP()
        end
    end)
    
    print("[Aimbot/ESP] Loaded. Press DELETE to hide/show UI.")
end

-- 키 입력 UI 로드 함수
local function loadKeyUI()
    local keyUI = Instance.new("ScreenGui")
    keyUI.Name = "AimbotKeyUI"
    key
